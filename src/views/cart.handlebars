  <h1>Carrito</h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Producto</th>
        <th>Precio</th>
        <th>Cantidad</th>
        <th>Total</th>
      </tr>
    </thead>
    <tbody>
      {{#each cart.products}}
    <tr>
        <td>{{product.title}}</td>
        <td>${{product.price}}</td>
        <td>{{quantity}}</td>
        <td><b>${{total}}</b></td>
    </tr>
      {{/each}}
    </tbody>
  </table>
